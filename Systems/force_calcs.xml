	</fcs_function>
	
	
	

	<!-- Z Vector -->
		
	<fcs_function name="T4T/forces/z-vector_wings_left-lbs">
		<function>
		<description>left wing z vector</description>
			<sum>
				<product name="T4T/forces/z-vector_wing_left_1-lbs">		
					<property>T4T/forces/normal_wing_left_1-lbs</property>
					<cos>
						<product>
							<value>0.017453</value>
							<property>T4T/aero/dihedral_wing_left_1-deg</property> <!-- angle of dihedral-deg -->
						</product>
					</cos>
					<cos>
						<product>
							<value>0.017453</value>
							<property>T4T/structure/wing_left_1/incidence-deg</property> <!-- angle of incidence-deg -->
						</product>
					</cos>				
				</product>
