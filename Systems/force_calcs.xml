	<!-- #### left #### -->
	

	<fcs_function name="T4T/forces/normal_wing_left_1-lbs">	
		<function>  <!-- force normal generated by this wing section -->
		<description>force normal wing_left_1</description> <!-- force normal -->
			<product>
				<value> -1 </value>
				<property>aero/function/kCLge</property> <!-- ground effect --> 
				<property>aero/effective-qbar_wing_left-psf</property> <!-- effective dynamic pressure --> 			
				<property>T4T/metrics/lift_modifier</property>
				<property>T4T/structure/wing_left_1/damage</property> <!-- damage condition -->
				<property>T4T/structure/wing_left_1/area</property> <!-- Area of wing section in sqft -->
				<property>T4T/coefficients/Cn_wing_left_1</property> <!-- airfoil Cn wing left -->
			</product>				
		</function>
	</fcs_function>	

