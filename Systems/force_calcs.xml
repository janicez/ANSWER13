<!-- wing normal forces -->	

	<fcs_function>
		<function> <!-- normal force generated by the wing -->
		<description>force normal left wing</description>
			<max>
				<value> 0.000001 </value>
				<sum>				
					<property>T4T/forces/normal_wing_left_1-lbs</property>
					<property>T4T/forces/normal_wing_left_2-lbs</property>
					<property>T4T/forces/normal_wing_left_3-lbs</property>
					<property>T4T/forces/normal_wing_left_4-lbs</property>
					<property>T4T/forces/normal_wing_left_5-lbs</property>
					<property>T4T/forces/normal_wing_left_6-lbs</property>
					<property>T4T/forces/normal_wing_left_7-lbs</property>
					<property>T4T/forces/normal_wing_left_8-lbs</property>
				</sum>
			</max>
		</function>
		<output>T4T/forces/normal_wing_left-lbs</output>		
	</fcs_function>

	<fcs_function>
		<function> <!-- normal force generated by the wing -->
		<description>force normal right wing</description>
			<max>
