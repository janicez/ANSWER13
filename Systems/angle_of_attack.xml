	<fcs_function name="T4T/aero/AAoA_h-stab-deg">
		<function>
        <description>Alpha Angle of Attack on h-stab due to main wing</description>
			<sum>
				<product>
					<table>
						<independentVar>aero/alpha-deg</independentVar>
						<tableData>
							-90		1.0
							-45		0.0
							45		0.0
							90		1.0
						</tableData>
					</table>
					<property>aero/alpha-deg</property>
				</product>
				<product>
					<table>
						<independentVar>aero/alpha-deg</independentVar>
						<tableData>
							-90		0.0
							-45		1.0
							45	 	1.0
							90		0.0
						</tableData>
					</table>
					<difference>
						<property>aero/alpha-deg</property>
						<property>T4T/structure/wing_left_1/incidence-deg</property>
					</difference>
				</product>
			</sum>
		</function>	
	</fcs_function>

