<system name="angle_of_attack">

 	<property type="double" value="0.1"> T4T/forces/normal_wing_left-lbs</property>
 	<property type="double" value="0.1"> T4T/forces/normal_wing_right-lbs</property>	
	
	<channel name="angle_of_attack">		
			
	<fcs_function name="T4T/aero/alpha_positive-rad">
		<function>
			<description>Alpha AoA as a positive rad</description>		
			<table>
				<independentVar><property>aero/alpha-rad</property></independentVar>
				<tableData>
					-6.283	0.0
					-4.71	1.57
					-3.142	0
					-1.57	1.57
					0		0.0
					1.57	1.57
					3.142	0
					4.71	1.57
					6.283	0.0
				</tableData>
			</table>
		</function>
	</fcs_function>			
							
	<fcs_function name="T4T/aero/beta_positive-rad">
		<function>
			<description>Beta AoA as a positive rad</description>	
			<table>
				<independentVar><property>aero/beta-rad</property></independentVar>
				<tableData>
					-6.283	0.0
					-4.71	1.57
					-3.142	0
					-1.57	1.57
					0		0.0
					1.57	1.57
					3.142	0
					4.71	1.57
					6.283	0.0
				</tableData>
			</table>
		</function>
	</fcs_function>
	
	<fcs_function name="aero/effective-qbar_wing_left-psf">
		<function>
			<description>The adjustment to the force normal generation caused by the airfoil being not perfectly aligned with the airflow</description>
			<sum>
				<product>
					<cos>
						<product>
							<value>0.017453</value>		
							<sum>
								<property>aero/beta-deg</property>
								<avg>
									<property>T4T/structure/wing_left_1/sweep-deg</property>
									<property>T4T/structure/wing_left_2/sweep-deg</property>
									<property>T4T/structure/wing_left_3/sweep-deg</property>
									<property>T4T/structure/wing_left_4/sweep-deg</property>
									<property>T4T/structure/wing_left_5/sweep-deg</property>
									<property>T4T/structure/wing_left_6/sweep-deg</property>
									<property>T4T/structure/wing_left_7/sweep-deg</property>
									<property>T4T/structure/wing_left_8/sweep-deg</property>
								</avg>
							</sum>
						</product>
					</cos>
					<property>aero/qbar-psf</property> <!-- dynamic pressure -->
					<table>
						<independentVar><property>aero/alpha-deg</property></independentVar>
						<tableData>
						-24		0
						-16		1
						-1		1
						9		1
						14		1
						27		0
						</tableData>
					</table>
				</product>
				<product>
					<property>aero/qbar-psf</property> <!-- dynamic pressure -->
					<table>
						<independentVar><property>aero/alpha-deg</property></independentVar>
						<tableData>
						-24		1
						-16		0
						-1		0
						9		0
						14		0
						27		1
						</tableData>
					</table>
				</product>
			</sum>
		</function>
	</fcs_function>	
	
